<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.howlinkpart3">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HowlinkPart3">
        <activity android:name=".ui.market.MarketList"></activity>
        <activity android:name=".ui.profile.PostActivity" />
        <activity android:name=".ui.profile.EmailChange" />
        <activity android:name=".ui.profile.PhoneChange" />
        <activity android:name=".ui.profile.PersonalSetting" />
        <activity android:name=".ui.profile.ExpandedImage" />
        <activity android:name=".ui.profile.EditProfile" />
        <activity android:name=".ui.signinworkflow.ProfileSetup">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>